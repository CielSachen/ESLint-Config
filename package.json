{
  "name": "@cielsachen/eslint-config",
  "version": "1.0.0",
  "description": "CielSachen's ESLint configs for Node.js projects.",
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "nodejs",
    "react",
    "tailwindcss",
    "typescript",
    "vite"
  ],
  "homepage": "https://github.com/CielSachen/ESLint-Config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CielSachen/ESLint-Config.git"
  },
  "license": "MIT",
  "author": "CielSachen",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./src/index.js"
    },
    "./base": {
      "types": "./types/base.d.ts",
      "default": "./src/base.js"
    },
    "./react": {
      "types": "./types/react.d.ts",
      "default": "./src/react.js"
    }
  },
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@eslint/css": "^0.8.1",
    "@eslint/js": "^9.28.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.5.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.7.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-perfectionist": "^4.14.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-regexp": "^2.8.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.2.0"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^22.15.30",
    "eslint": "^9.28.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "peerDependencies": {
    "eslint": "9.x",
    "typescript": "5.x",
    "typescript-eslint": "8.x"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "engines": {
    "node": ">=22.11.0",
    "pnpm": "10.x"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
